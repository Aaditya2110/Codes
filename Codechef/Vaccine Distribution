#include <stdio.h>
#include <math.h>
int main(void) {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b;
	    float c=0,d=0;
	    scanf("%d %d",&a,&b);
	    int arr[a];
	    for(int i =0;i<a;i++){
	        scanf("%d",&arr[i]);
	        if(arr[i]<=9||arr[i]>=80){
	            c++;
	        }else{
	            d++;
	        }
	    }
	    int x = ceil(c/b)+ceil(d/b);
	    printf("%d\n",x);

	}
	return 0;
}

